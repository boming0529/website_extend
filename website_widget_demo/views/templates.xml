<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Assets Frontend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/website_widget_demo/static/src/scss/heroes.scss"/>
            <script type="text/javascript" src="/website_widget_demo/static/src/js/heroes.component.js"/>
